#include<stdio.h>
int main()
{
    int a[50],p;
    p=great(a,50);
    printf("Smallest number is %d",p);
    return 0;
}
int great(int b[],int x)
{
    int i,g,n;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter the %d of element :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    g=b[0];
    for(i=1;i<n;i++)
    {
        if(g>b[i])
            g=b[i];
    }
    return g;
}


Ans3.

#include<stdio.h>
int main()
{
    int a[50];
    great(a,50);
    return 0;
}
void great(int b[],int x)
{
    int i,j,n,temp;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter the %d of element :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(b[i]>b[j])
            {
                temp=b[i];
                b[i]=b[j];
                b[j]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
        printf("%d ",b[i]);
}


Ans6.


#include<stdio.h>
int main()
{
    int a[50];
    reverse(a,50);
    return 0;
}
void reverse(int b[],int x)
{
    int n,i;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter %d number of elemnt :\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&b[i]);
    printf("Reverse order is\n");
    for(i=n-1;i>=0;i--)
        printf("%d ",b[i]);
}


Ans7.


#include<stdio.h>
int main()
{
    int a[50];
    duplicate(a,50);
    return 0;
}
void duplicate(int b[],int x)
{
    int n,i,j,count=0;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter %d number of elemnt :\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&b[i]);
    printf("Total duplicate element is :\n");
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<=n;j++)
        {
            if(b[i]==b[j])
                count++;
        }
    }
    printf("%d",count);
}


Ans4.


#include<stdio.h>
int main()
{
    int a[50];
    duplicate(a,50);
    return 0;
}
void duplicate(int b[],int x)
{
    int n,i,j,d,temp,s;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter %d number of elemnt :\n",n);
    for(i=0;i<n;i++)
        scanf("%d",&b[i]);
    printf("Enter the n position value :");
    scanf("%d",&s);
    printf("Enter the d direction value :");
    scanf("%d",&d);
    for(j=0;j<d;j++)
    {
        temp=b[0];
        for(i=0;i<n-1;i++)
        {
            b[i]=b[i+1];
        }
        b[n-1]=temp;
    }
    for(i=0;i<n;i++)
        printf("%d ",b[i]);
}



Ans1.

#include<stdio.h>
int main()
{
    int a[50],p;
    p=great(a,50);
    printf("Greatest number is %d",p);
    return 0;
}
int great(int b[],int x)
{
    int i,g,n;
    printf("Enter the number of element :");
    scanf("%d",&n);
    printf("Enter the %d of element :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    g=b[0];
    for(i=1;i<n;i++)
    {
        if(g<b[i])
            g=b[i];
    }
    return g;
}



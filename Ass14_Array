Ans1.

#include<stdio.h>
int main()
{
    int i,a[10],s=0;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
        s=s+a[i];
    }
    printf("Total sum is %d",s);
    return 0;
}

Ans.2

#include<stdio.h>
int main()
{
    int i,a[10],s=0;
    float avg;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
        s=s+a[i];
    }
    avg=s/10.0;
    printf("Average is %f",avg);
    return 0;
}


Ans.3

#include<stdio.h>
int main()
{
    int i,a[10],e=0,o=0;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]%2==0)
           e=e+a[i];
        else
           o=o+a[i];
    }
    printf("Total sum of even numbers is %d",e);
    printf("\nTotal sum of odd numbers is %d",o);
    return 0;
}


Ans.4

#include<stdio.h>
int main()
{
    int i,a[10],g;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    g=a[0];
    for(i=0;i<10;i++)
    {
        if(g<a[i])
            g=a[i];
    }
    printf("Greatest number is %d",g);
    return 0;
}



#include<stdio.h>
int main()
{
    int i,a[10],g;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
     g=a[0];
    for(i=0;i<10;i++)
    {
        if(g>a[i])
            g=a[i];
    }
    printf("Smallest number is %d",g);
    return 0;
}


Ans6.

#include<stdio.h>
int main()
{
    int i,j,a[10],temp;
    printf("Enter the 10 number of elements :\n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("\nSorted array is\n");
    for(i=0;i<9;i++)
    {
        for(j=i+1;j<10;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    for(i=0;i<10;i++)
        printf("%d ",a[i]);
        return 0;
}


Ans7.

#include<stdio.h>
int main()
{
    int i,j,a[50],temp,n;
    printf("Enter the number of elements :\n");
    scanf("%d",&n);
    printf("Enter the %d of elements :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<10;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    printf("Second largest element is %d",a[n-2]);
        return 0;
}


Ans8.

#include<stdio.h>
int main()
{
    int i,j,a[50],temp,n;
    printf("Enter the number of elements :\n");
    scanf("%d",&n);
    printf("Enter the %d of elements :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<10;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    printf("Second smallest element is %d",a[1]);
        return 0;
}


Ans9.

#include<stdio.h>
int main()
{
    int i,n,a[50];
    printf("Enter the number of elements :\n");
    scanf("%d",&n);
    printf("Enter the %d of elements :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Reverse order is:\n");
    for(i=n-1;i>=0;i--)
        printf("%d ",a[i]);
        return 0;
}

Ans10.

#include<stdio.h>
int main()
{
    int i,n,a[50],b[50];
    printf("Enter the number of elements :\n");
    scanf("%d",&n);
    printf("Enter the %d of elements :\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
        printf("%d ",b[i]=a[i]);
        return 0;
}

